{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Meghrazi-Admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Meghrazi-Admin",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/animate.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/fonts.css",
              "src/assets/css/flaticon.css",
              "src/assets/css/font-awesome.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/owl.theme.default.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/dropify.min.css",
              "src/assets/css/nice-select.css",
              "src/assets/css/jquery-ui.min.css",
              "src/assets/css/reset.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.3.1.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/modernizr.js",
              "src/assets/js/jquery.menu-aim.js",
              "src/assets/js/plugin.js",
              "src/assets/js/owl.carousel.js",
              "src/assets/js/jquery.countTo.js",
              "src/assets/js/dropify.min.js",
              "src/assets/js/jquery-ui.js",
              "src/assets/js/Chart.bundle.js",
              "src/assets/js/jquery.inview.min.js",
              "src/assets/js/jquery.magnific-popup.js",
              "src/assets/js/jquery.nice-select.min.js",
              "src/assets/js/imagesloaded.pkgd.min.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/js/custom.js",
              "src/assets/js/ChartJS.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Meghrazi-Admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Meghrazi-Admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Meghrazi-Admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Meghrazi-Admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Meghrazi-Admin:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "Meghrazi-Admin"
}
